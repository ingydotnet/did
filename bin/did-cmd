#!/usr/bin/env bash

source "$(dirname $BASH_SOURCE)/../lib/use.bash"
use GetOpt

GETOPT_SPEC="\
  did cmd [<options>] <did-name>

See 'did help cmd' for more help.

Options:
--
"

main() {
  GETOPT_ARGS='+did-name' \
    getopt "$@"
  normalize-did-name
  eval "$(did id --yaml "$did_name" | yaml2bash)"
  echo "docker run $cmd_main_run"
}

[[ $0 != "$BASH_SOURCE" ]] || main "$@"

# vim: set ft=sh lisp:
